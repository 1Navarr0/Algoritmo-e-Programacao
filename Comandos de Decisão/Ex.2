algoritmo "02 - Decisão"

var
    num1, num2 : inteiro

inicio
    escreval("Informe um número inteiro: ")
    leia(num1)
    escreval("Informe um outro número inteiro ")
    leia(num2)
    se num1 > num 2 entao
        escreval("O número", num1, "é o maior")
    senao
        escreval("O número", num2, "é o maior")
    fimse
fimalgoritmo
