algoritmo "08 - Repetição"

var
    num : inteiro
    multi, i : inteiro

inicio
    
    escreva("Informe um número maior que zero: ")
    leia(num)

    para i de 0 ate 10 faca
        multi <- numero * contador
        escreval(numero, " x", contador, " =", multi)
    fimpara

fimalgoritmo
        
