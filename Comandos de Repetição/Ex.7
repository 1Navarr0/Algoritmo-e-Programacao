algoritmo "07 - Repetição"

var
    num : inteiro
    teste : inteiro
    i : inteiro

inicio
    soma <- 0
    escreva("Informe um número maior que zero: ")
    leia(num)
    para i de 1 ate numero faca
        se numero % i = 0 entao
            teste <- teste + 1
        fimse
    fimpara

    se teste > 2 entao
        escreval("O número", numero, " não é primo")
    senao
        escreval("O número", numero, " é primo")
    fimse
fimalgoritmo
        
