Algoritmo "05 - Operadores"

Var
    valorDolar, dolares, reais : real

Inicio

   escreval("Informe a cotação do dólar atual: ")
   leia(valorDolar)
   escreval("Informe a quantiade de dólares que você possui: ")
   leia(dolares)

    reais <- valorDolar * dolares
    escreval("A conversão em real fica: R$", reais)

fimalgoritmo
