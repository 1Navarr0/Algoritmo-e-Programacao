Algoritmo "004 - Operadores"

Var
    variavelA, variavelB : logico

inicio

    variavelA <- falso
    variavelB <- verdadeiro
    escreval("A variável A é:", variavelA)
    escreval("A variável B é:", variavelB)


    variavelA <- nao(variavelA)
    variavelB <- nao(variavelB)
    escreval("Após as trocas")
    escreval("A variável A é:", variavelA)
    escreval("A variável B é:", variavelB)

fimalgoritmo


    
