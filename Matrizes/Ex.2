algoritmo "semnome"

var

   mat: vetor[1..10,1..10] de inteiro
   l, c : inteiro

inicio

   para l de 1 ate 10 faca
      para c de 1 ate 10 faca
         mat[l,c] <- randi(100)
      fimpara
   fimpara

   para l de 1 ate 10 faca
      escreval("")
      para c de 1 ate 10 faca
         escreva(mat[l,c])
      fimpara
   fimpara

fimalgoritmo
